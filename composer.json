{
    "name": "rap2h/mysql-explain-explain",
    "description": "Mysql Explain Explainer",
    "license": "MIT",
    "authors": [
        {
            "name": "Raphael Ht",
            "email": "raphaelht@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "3.1.*",
        "league/plates": "1.0.*",
        "jasny/db-mysql": "1.*",
        "phpmyadmin/sql-parser": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Rap2hpoutre\\MySQLExplainExplain\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "bin": [
        "bin/explain"
    ]
}
